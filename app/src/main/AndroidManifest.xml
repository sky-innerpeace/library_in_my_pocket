<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.techtown.home">

    <uses-permission android:name="android.permission.INTERNET" /> <!-- 인터넷 권한 -->
    <uses-permission android:name="android.permission.CAMERA" /> <!-- 카메라 권한 -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <uses-feature android:name="android.hardware.camera" /> <!-- 카메라 권한 -->
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_unsettled_book"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".activity.BookDetailsActivity"></activity>
        <activity android:name=".activity.BookPostActivity"/>
        <activity android:name=".activity.SearchResultActivity" />
        <activity android:name=".activity.SearchActivity" />
        <activity android:name=".activity.MarathonActivity" />
        <activity android:name=".morphemeAnalysis" />
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.LoginActivity"/>
        <activity android:name=".activity.IntroActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.MemberInfoActivity"/>
        <activity android:name=".activity.MemberInitActivity" />
        <activity android:name=".activity.BasicActivity" />
        <activity android:name=".activity.CameraActivity" />
        <activity android:name=".activity.GalleryActivity" />
        <activity android:name=".activity.PostActivity" />
        <activity android:name=".sns.SnsMainActivity" />
        <activity android:name=".sns.SnsPostActivity"/>
        <activity android:name=".sns.SnsWritePostActivity" />
        <activity android:name=".activity.ReviewMainActivity" />
        <activity android:name=".activity.EditProfileActivity"/>
        <activity android:name=".activity.SignUpActivity"/>
    </application>

</manifest>